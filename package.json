{
  "name": "@ac-ui/react-components",
  "private": true,
  "engines": {
    "node": "10.0.0"
  },
  "version": "1.0.0",
  "jsnext:main": "dist/es/index.js",
  "main": "dist/commonjs/index.js",
  "module": "dist/es/index.js",
  "scripts": {
    "test": "BABEL_ENV=testing jest",
    "postinstall": "npm run build",
    "build": "npm run build:commonjs && npm run build:es",
    "build:commonjs": "./node_modules/.bin/cross-env NODE_ENV=build babel src -d dist/commonjs --copy-files",
    "build:es": "./node_modules/.bin/cross-env NODE_ENV=build-es babel src -d dist/es --copy-files",
    "styleguide": "npm run styleguide:server",
    "styleguide:server": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "lint": "./node_modules/.bin/eslint src",
    "pretest": "./node_modules/.bin/eslint --fix src"
  },
  "dependencies": {
    "@ac-ui/design-system": "git+ssh://git@gitlab.com:acbr/internal/ac-ui/design-system.git",
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-transform-flow-strip-types": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "classnames": "^2.2.6",
    "cross-env": "^5.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.0",
    "flow-bin": "^0.98.1",
    "jest": "^24.8.0",
    "lint-staged": "^8.1.7",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "reactstrap": "^8.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-plugin-flowtype": "^3.9.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "file-loader": "^3.0.1",
    "flow-runtime": "^0.17.0",
    "husky": "^2.3.0",
    "node-sass": "^4.12.0",
    "react-styleguidist": "^9.0.9",
    "resolve-url-loader": "^3.1.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
